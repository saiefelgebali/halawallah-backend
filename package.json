{
  "name": "halawallah-backend",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "start": "node -r dotenv/config lib/index.js",
    "dev": "nodemon -r dotenv/config src/index.ts",
    "test": "jest",
    "build": "tsc -p .",
    "makemigrate": "npx knex migrate:make init -x ts --migrations-directory src/database/migrations/",
    "migrate": "npx knex migrate:latest --knexfile src/database/config.ts",
    "makeseed": "npx knex seed:make --knexfile src/database/config.ts new_seed",
    "seed": "npx knex seed:run --knexfile src/database/config.ts"
  },
  "keywords": [],
