    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "sharp": "^0.28.1",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
